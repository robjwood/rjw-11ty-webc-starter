<nav class="background-color:black">
  <ul class="o-box o-cluster o-centre c-nav-primary" role="list">
    {% for item in navigation.top %}
    <li>
      <a
        href="{{ item.url }}"
        {{ helpers.getLinkActiveState(item.url, page.url) | safe }}
      >
        {{ item.text }}
      </a>
    </li>
    {% endfor %}
  </ul>
</nav>